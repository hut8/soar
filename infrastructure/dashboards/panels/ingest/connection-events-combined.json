{
  "datasource": {
    "type": "prometheus",
    "uid": "prometheus"
  },
  "fieldConfig": {
    "defaults": {
      "color": {
        "mode": "palette-classic"
      },
      "custom": {
        "axisBorderShow": false,
        "axisCenteredZero": false,
        "axisColorMode": "text",
        "axisLabel": "",
        "axisPlacement": "auto",
        "barAlignment": 0,
        "barWidthFactor": 0.6,
        "drawStyle": "line",
        "fillOpacity": 10,
        "gradientMode": "none",
        "hideFrom": {
          "legend": false,
          "tooltip": false,
          "viz": false
        },
        "insertNulls": false,
        "lineInterpolation": "linear",
        "lineWidth": 1,
        "pointSize": 5,
        "scaleDistribution": {
          "type": "linear"
        },
        "showPoints": "never",
        "showValues": false,
        "spanNulls": false,
        "stacking": {
          "group": "A",
          "mode": "none"
        },
        "thresholdsStyle": {
          "mode": "off"
        }
      },
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": 0
          }
        ]
      },
      "unit": "short"
    },
    "overrides": [
      {
        "matcher": {
          "id": "byRegexp",
          "options": ".*Failed.*|.*Error.*|.*Ended.*|.*Timeout.*"
        },
        "properties": [
          {
            "id": "color",
            "value": {
              "fixedColor": "red",
              "mode": "fixed"
            }
          }
        ]
      }
    ]
  },
  "options": {
    "legend": {
      "calcs": [
        "lastNotNull"
      ],
      "displayMode": "table",
      "placement": "bottom",
      "showLegend": true
    },
    "tooltip": {
      "hideZeros": false,
      "mode": "multi",
      "sort": "desc"
    }
  },
  "pluginVersion": "12.2.1",
  "targets": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "aprs_connection_established_total{component=\"ingest\",environment=~\"$environment\"}",
      "legendFormat": "OGN Established",
      "range": true,
      "refId": "A"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "beast_connection_established_total{component=\"ingest\",environment=~\"$environment\"}",
      "legendFormat": "Beast Established",
      "range": true,
      "refId": "B"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "sbs_connection_established_total{component=\"ingest\",environment=~\"$environment\"}",
      "legendFormat": "SBS Established",
      "range": true,
      "refId": "C"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "aprs_connection_failed_total{component=\"ingest\",environment=~\"$environment\"}",
      "legendFormat": "OGN Failed",
      "range": true,
      "refId": "D"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "beast_connection_failed_total{component=\"ingest\",environment=~\"$environment\"}",
      "legendFormat": "Beast Failed",
      "range": true,
      "refId": "E"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "sbs_connection_failed_total{component=\"ingest\",environment=~\"$environment\"}",
      "legendFormat": "SBS Failed",
      "range": true,
      "refId": "F"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "aprs_connection_ended_total{component=\"ingest\",environment=~\"$environment\"}",
      "legendFormat": "OGN Ended",
      "range": true,
      "refId": "G"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "beast_timeout_total{component=\"ingest\",environment=~\"$environment\"}",
      "legendFormat": "Beast Timeout",
      "range": true,
      "refId": "H"
    }
  ],
  "title": "Connection Events",
  "type": "timeseries"
}
