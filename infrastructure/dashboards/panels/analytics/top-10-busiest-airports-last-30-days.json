{
  "datasource": {
    "type": "postgres",
    "uid": "${postgres_datasource}"
  },
  "description": "Top 10 busiest airports by total flight count in the last 30 days",
  "fieldConfig": {
    "defaults": {
      "color": {
        "mode": "thresholds"
      },
      "custom": {
        "align": "auto",
        "cellOptions": {
          "type": "auto"
        },
        "inspect": false
      },
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          }
        ]
      }
    },
    "overrides": [
      {
        "matcher": {
          "id": "byName",
          "options": "Total Operations"
        },
        "properties": [
          {
            "id": "custom.width",
            "value": 140
          },
          {
            "id": "custom.cellOptions",
            "value": {
              "type": "color-background"
            }
          },
          {
            "id": "color",
            "value": {
              "mode": "continuous-GrYlRd"
            }
          }
        ]
      }
    ]
  },
  "options": {
    "cellHeight": "sm",
    "footer": {
      "countRows": false,
      "fields": "",
      "reducer": [
        "sum"
      ],
      "show": false
    },
    "showHeader": true,
    "sortBy": []
  },
  "pluginVersion": "10.0.0",
  "targets": [
    {
      "datasource": {
        "type": "postgres",
        "uid": "${postgres_datasource}"
      },
      "editorMode": "code",
      "format": "table",
      "rawQuery": true,
      "rawSql": "SELECT\n  MAX(airport_ident) as \"Airport\",\n  MAX(airport_name) as \"Name\",\n  SUM(departure_count) as \"Departures\",\n  SUM(arrival_count) as \"Arrivals\",\n  SUM(departure_count + arrival_count) as \"Total Operations\"\nFROM airport_analytics_daily\nWHERE date >= CURRENT_DATE - 30\nGROUP BY airport_id\nORDER BY SUM(departure_count + arrival_count) DESC\nLIMIT 10",
      "refId": "A",
      "sql": {
        "columns": [],
        "groupBy": [],
        "limit": 50
      }
    }
  ],
  "title": "Top 10 Busiest Airports (Last 30 Days)",
  "type": "table"
}
