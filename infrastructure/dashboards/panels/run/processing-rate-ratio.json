{
  "datasource": {
    "type": "prometheus",
    "uid": "prometheus"
  },
  "description": "Ratio of data-time processed per wall-clock second. >1 means catching up, <1 means falling behind, =1 means keeping pace.",
  "fieldConfig": {
    "defaults": {
      "color": {
        "mode": "palette-classic"
      },
      "custom": {
        "axisBorderShow": false,
        "axisCenteredZero": false,
        "axisColorMode": "text",
        "axisLabel": "ratio",
        "axisPlacement": "auto",
        "barAlignment": 0,
        "barWidthFactor": 0.6,
        "drawStyle": "line",
        "fillOpacity": 10,
        "gradientMode": "none",
        "hideFrom": {
          "legend": false,
          "tooltip": false,
          "viz": false
        },
        "insertNulls": false,
        "lineInterpolation": "smooth",
        "lineWidth": 2,
        "pointSize": 5,
        "scaleDistribution": {
          "type": "linear"
        },
        "showPoints": "never",
        "spanNulls": false,
        "stacking": {
          "group": "A",
          "mode": "none"
        },
        "thresholdsStyle": {
          "mode": "line"
        }
      },
      "decimals": 2,
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "red",
            "value": null
          },
          {
            "color": "green",
            "value": 1
          }
        ]
      },
      "min": 0,
      "unit": "none"
    },
    "overrides": []
  },
  "options": {
    "legend": {
      "calcs": [
        "lastNotNull",
        "mean"
      ],
      "displayMode": "table",
      "placement": "bottom",
      "showLegend": true
    },
    "tooltip": {
      "hideZeros": false,
      "mode": "single",
      "sort": "none"
    }
  },
  "pluginVersion": "12.2.1",
  "targets": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "clamp(avg_over_time(socket_router_processing_rate_ratio{component=\"run\",environment=\"$environment\"}[5m]), 0, 10)",
      "legendFormat": "Processing Rate Ratio",
      "range": true,
      "refId": "A"
    }
  ],
  "title": "Processing Rate Ratio",
  "type": "timeseries"
}
