{
  "datasource": {
    "type": "prometheus",
    "uid": "prometheus"
  },
  "description": "API error rate (errors per second)",
  "fieldConfig": {
    "defaults": {
      "color": {
        "mode": "thresholds"
      },
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "yellow",
            "value": 0.01
          },
          {
            "color": "red",
            "value": 0.1
          }
        ]
      },
      "unit": "reqps"
    },
    "overrides": []
  },
  "options": {
    "colorMode": "value",
    "graphMode": "area",
    "justifyMode": "auto",
    "orientation": "auto",
    "reduceOptions": {
      "values": false,
      "calcs": [
        "lastNotNull"
      ],
      "fields": ""
    },
    "textMode": "auto"
  },
  "pluginVersion": "10.0.0",
  "targets": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "expr": "rate(coverage_api_errors_total[5m])",
      "refId": "A"
    }
  ],
  "title": "API Error Rate",
  "type": "stat"
}
